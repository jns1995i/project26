];

const shades = ['50','100','200','300','400','500','600','700','800','900','950'];

const container = document.getElementById('colorPalette');
container.className = 'flex row wrap gap10 justifyCenter';

colors.forEach(color => {
  shades.forEach(shade => {
    const className = `bg${color}${shade}`;

    const isDark = parseInt(shade) >= 500;
    const textContrast = isDark ? 'white' : 'black';

    const block = document.createElement('div');
    // Added 'inlineBlock' to ensure they don't force full-width
    block.className = 'width0 alignCenter corner5 margin5'; 
    
    block.innerHTML = `
      <section class="hpx40 wpx80 corner5 padding10 ${className} ${textContrast} size10 mono">
        ${className}
      </section>
    `;
    container.appendChild(block);
  });
});
</script>

